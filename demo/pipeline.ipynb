from google import generativeai as gen
from PIL import Image

gen.configure(api_key="PASTE_KEY_HERE")

img = Image.open("demo/input_sample/meal_sample_01.jpg")

meal_prompt = open("model/prompts/prompt_meal_analysis.md").read()
meal_response = gen.GenerativeModel("gemini-pro-vision").generate_content([meal_prompt, img])

print(meal_response.text)

reflection = "I felt tired and craved something sweet."
emotion_prompt = open("model/prompts/prompt_emotion_analysis.md").read()
emotion_response = gen.GenerativeModel("gemini-pro").generate_content(f"{emotion_prompt}\n{reflection}")

print("\n", emotion_response.text)
